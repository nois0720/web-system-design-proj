    <script>
        function getLevel(event) {
            var form = document.getElementById('_form');
            var tr = event.target;
            var target = tr.parentNode.childNodes[7];
            var createTime = target.innerText;
            var el;

            console.log("create Time is : " + createTime);

            el = document.createElement("input");
            el.type = "hidden";
            el.name = "createTime";
            el.value = createTime;
            form.appendChild(el);
            form.setAttribute('action', '/game/start');
            form.submit();
        }
    </script>
