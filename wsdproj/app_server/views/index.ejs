<div class="container" id="levelDesignMain">
    <table class="table">
        <thead class="thead-inverse">
        <tr>
            <th>NO</th>
            <th>Level Name</th>
            <th>Level Designer</th>
            <th></th>
        </tr>
        </thead>

        <tbody>
        <% for(var i = 0; i < levelList.length; i++){ %>
        <tr onclick="getLevel(event);">
            <th scope="row"> <%= i + 1 %></th>
            <td class="td_first">
                <%= levelList[i].levelName %>
            </td>
            <td class="td_third">
                <%= levelList[i].levelDesigner %>
            </td>
            <td>
                <%= levelList[i].createTime %>
            </td>
        </tr>
        <% } %>
        </tbody>
    </table>
    <form method="get" id="_form">
    </form>
